<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Automation Training Tests" verbose="1">
	<listeners>
		<listener class-name="com.br.utils.TestExecutionListener"></listener>
	</listeners>
	<tests>
		<test name="Win/Chrome tests">
			<parameters>
				<parameter name="platform" value="WIN" />
				<parameter name="browser" value="chrome" />
